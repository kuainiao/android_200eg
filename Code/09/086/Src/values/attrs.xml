<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WaveControlLayout">
        <attr name="wa_color" format="color" />
        <attr name="wa_strokeWidth" format="dimension"/>
        <attr name="wa_radius" format="dimension"/>
        <attr name="wa_duration" format="integer"/>
        <attr name="wa_rippleAmount" format="integer"/>
        <attr name="wa_scale" format="float"/>
        <attr name="wa_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
</resources>